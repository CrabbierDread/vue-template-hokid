{
  "presets": [
    ["env", { "modules": false }],
    "stage-2",
    "flow"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-decorators-legacy",
    "flow-runtime",
    "transform-runtime"
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2", "flow"],
      "plugins": [ "istanbul" ]
    }
  }
}
